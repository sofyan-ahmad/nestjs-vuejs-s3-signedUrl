{
  "name": "presigned-s3-service",
  "version": "0.0.0",
  "author": "sofyan.h.ahmad@gmail.com",
  "private": true,
  "publishConfig": {
    "access": "private"
  },
  "scripts": {
    "serve": "nodemon",
    "serve:prod": "node dist/main.js",
    "local": "sls offline --port 3004 --skipCacheInvalidation --stage local --env local",
    "offline": "yarn local",
    "build": "rm -rf dist && tsc --p tsconfig.json -d",
    "build:deploy": "yarn build && sls deploy",
    "deploy": "sls deploy",
    "deploy:dev": "sls deploy --stage dev --env development",
    "deploy:staging": "sls deploy --stage staging --env staging",
    "deploy:prod": "sls deploy --stage prod --env production",
    "barrels": "barrelsby --delete -d ./src --singleQuotes",
    "crate_domain:dev": "sls create_domain --stage dev",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "migration:barrels": "barrelsby --delete -d ./src/migrations --singleQuotes",
    "precommit": "yarn e2e:coverage",
    "lint": "tslint -p tsconfig.json -c tslint.json \"src/**/*.ts\" \"e2e/**/*.ts\" -e \"src/migrations/**/*.ts\" \"*spec.ts\"",
    "lint:fix": "yarn lint --fix",
    "e2e": "jest --config=jest-e2e.json --forceExit --detectOpenHandles",
    "e2e:watch": "jest --watch --config=jest-e2e.json --detectOpenHandles",
    "e2e:coverage": "jest --config=jest-e2e.json --coverage --coverageDirectory=./coverage --detectOpenHandles"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@nestjs/common": "^7.0.8",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^7.0.8",
    "@nestjs/platform-express": "^7.0.8",
    "@nestjs/swagger": "^4.5.2",
    "@nestjs/typeorm": "^7.0.0",
    "@nestjsx/crud": "^4.4.5",
    "@nestjsx/crud-typeorm": "^4.4.5",
    "aws-lambda": "^1.0.5",
    "aws-sdk": "^2.658.0",
    "aws-serverless-express": "^3.3.8",
    "axios": "^0.19.2",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.1",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.1.0",
    "rxjs": "^6.5.5",
    "typeorm": "^0.2.24",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.0.8",
    "@types/aws-lambda": "^8.10.50",
    "@types/aws-serverless-express": "^3.3.3",
    "@types/cors": "^2.8.6",
    "@types/jest": "^25.2.1",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/lodash": "^4.14.150",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.0",
    "@types/socket.io": "^2.1.4",
    "@types/uuid": "^7.0.2",
    "@types/ws": "^7.2.4",
    "barrelsby": "^2.2.0",
    "jest": "^25.4.0",
    "nodemon": "^2.0.3",
    "serverless-domain-manager": "^3.3.1",
    "serverless-dotenv-plugin": "^2.4.2",
    "serverless-offline": "^6.1.4",
    "serverless-plugin-include-dependencies": "^4.1.0",
    "serverless-plugin-optimize": "^4.1.4-rc.1",
    "sqlite3": "^4.1.1",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.1.4",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  }
}
